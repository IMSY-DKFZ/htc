{
    "lightning_class": "htc.tissue_atlas.median_pixel.LightningMedianPixel>LightningMedianPixel",
    "label_mapping": "htc.tissue_atlas.settings_atlas>label_mapping",
    "input": {
        "data_spec": "tissue-atlas_loocv_test-8_seed-0_cam-118.json",
        "normalization": "L1",
        "n_channels": 100,
        "oversampling": false,
        "epoch_size": 10000000
    },
    "optimization": {
        "optimizer": {
            "name": "Adam",
            "lr": 0.0001,
            "weight_decay": 0
        },
        "lr_scheduler": {
            "name": "ExponentialLR",
            "gamma": 0.9
        }
    },
    "model": {
        "activation_function": "elu",
        "class_weight_method": "softmin",
        "softmin_scaling": -2.0,
        "normalization": "BatchNorm",
        "dropout": 0.2
    },
    "dataloader_kwargs": {
        "batch_size": 20000,
        "num_workers": 12
    },
    "trainer_kwargs": {
        "max_epochs": 10,
        "precision": 16,
        "devices": 1,
        "accelerator": "gpu"
    },
    "swa_kwargs": {
        "annealing_epochs": 0
    },
    "validation": {
        "checkpoint_metric": "accuracy",
        "dataset_index": 0
    }
}
